<%= fields_for "user[meta_fields_attributes][#{@temp_id}]", @meta_field do |mf| %>
  <div class="form-group">
    <div class="row">
      <div class="col-6">
        <%= mf.text_field :name, class: "form-control", placeholder: 'Name' %>
      </div>
      <div class="col-6">
        <%= mf.text_field :value, class: "form-control", placeholder: 'Value' %>
      </div>
    </div>
  </div>
<% end %>
